<Window x:Class="MedocScanner.Views.ScannePatientWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MedocScanner.Views"
        mc:Ignorable="d"
        Title="ScannePatientWindow" Height="170" Width="250">
    <StackPanel>
        <Label Content="scannez votre patient ici SVP " Margin=" 0 0 0 0" Width="167"/>
        <TextBox x:Name="IdPatient" KeyDown="IdPatient_KeyDown" Margin=" 0 10 0 0" Height="20" Width="200"/>
        <Button x:Name="Btn_Cancel" Content="annuler" Width="100" Margin=" 0 35 0 0" Click="Btn_Cancel_Click" />
    </StackPanel>
</Window>
